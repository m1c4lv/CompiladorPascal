# programa con operaciones
programa cuatro:
	constantes
		a = 30;
		b = 20;
	variables
		suma : entero;
		resultado : entero;
		
		conjuncion : booleano;
		resultadoLogico : booleano;


comienzo
	suma = a + b;
	resultado = suma * 10 + 4;
	
	escribir(suma);
	escribir(resultado);

	conjuncion = cierto y falso;
	conjuncion = no conjuncion;
	resultadoLogico = conjuncion y (a < b);

	si conjuncion entonces:
		escribir("cierto");
	sino:
		escribir("falso");
	fin si;	
	

fin.

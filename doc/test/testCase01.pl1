# programa con operaciones
programa cinco:
	tipos
		tvector = vector [1..5] de entero;
	variables
		v : tvector;
		a: entero;
		b: entero;


comienzo

	v[1] = 1;
	v[3] = 3;
	v[5] = 5;

	v[2] = v[1] + 1;

	v[4] = v[3] * v[5] + 10;


	a = v[v[3]];
	b = v[4];
	escribir(a);
	escribir();
	escribir(b);
	

fin.